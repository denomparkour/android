<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Atom-OS remote -->
  <remote name="atom"
          fetch="https://github.com/Atom-OS"
          revision="refs/heads/pie"
          sync-j="8" />

  <remote name="github"
          fetch="https://github.com/" />

  <remote name="gitlab"
          fetch="https://gitlab.com/" />
  
   <remote name="pixel-gitlab"
          fetch="https://gitlab.com/PixelExperience"
          revision="pie" />

  <!-- Removed projects -->	   
  <include name="remove.xml" />

  <!-- Imported projects -->
  <include name="atom/build.xml" />
  <include name="atom/devices.xml" />
  <include name="atom/external.xml" />
  <include name="atom/frameworks.xml" />
  <include name="atom/hals.xml" />
  <include name="atom/packages.xml" />
  <include name="atom/prebuilts.xml" />
  <include name="atom/system.xml" />
  <include name="atom/vendors.xml" />

</manifest>
